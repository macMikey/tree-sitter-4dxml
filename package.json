{
  "name": "tree-sitter-4dpreferences",
  "version": "0.2.0",
  "description": "Atom language support for 4D",
  "main": "index.js",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "test": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miyako/tree-sitter-4dpreferences.git"
  },
  "tree-sitter": [
    {
      "scope": "4dpreferences",
      "file-types": [
        "4dpreferences",
        "4dsettings"
      ]
    }
  ],
  "keywords": [
    "4D",
    "ORDA",
    "tree-sitter"
  ],
  "authors": [
    "Max Brunsfeld <maxbrunsfeld@gmail.com>",
    "Ashi Krishnan <queerviolet@github.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miyako/tree-sitter-4dpreferences/issues"
  },
  "homepage": "https://github.com/miyako/tree-sitter-4dpreferences#readme",
  "dependencies": {
    "nan": "^2.14.0",
    "node-gyp": "^6.1.0",
    "tree-sitter": "^0.16.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.3"
  }
}
